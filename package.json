{
  "name": "bib",
  "version": "0.0.2",
  "description": "Cartographie des centres de recherche en Sciences Politiques, réalisé par la bibliothèque et le médialab de Sciences Po Paris ainsi que ByteClub",
  "repository": {
    "type": "git",
    "url": "https://github.com/SciencesPoDRIS/CartoScPo.git"
  },
  "main": "index.html",
  "scripts": {
    "start": "gulp",
    "lint": "eslint app",
    "parse": "npm-run-all -p parse:*",
    "parse:data": "node app/data/script_data_csv_to_json",
    "~obsolete~parse:metadata": "node app/data/script_metadata_csv_to_json",
    "bo:watch": "npm-run-all -p bo:watch:*",
    "bo:watch:client": "webpack -d",
    "bo:watch:server": "DEBUG=express,mongo,mongoose,mailer,redis nodemon --watch server server/index.js",
    "bo:serve": "DEBUG=express node server/index.js",
    "bo:populate": "node bin/populate-mongo-db.js",
    "bo:dump": "node bin/dump-mongo-db.js",
    "bo:build": "NODE_ENV=production webpack -p",
    "bo:analyze-bundle": "ANALYZE_BUNDLE=1 npm run bo:build",
    "docker": "UID=$(id -u) GID=$(id -g) docker-compose -f docker-compose.dev.yml up",
    "maildev": "maildev -w 3025 -s 25000 --verbose",
    "prettier": "prettier-eslint --write \"app/**/*.js\" \"back-office/**/*.js\" \"server/**/*.js\" \"bin/*.js\""
  },
  "keywords": [
    "Science",
    "Politque",
    "Bibliothèque",
    "SciencesPo",
    "médialab",
    "angular",
    "Leaflet",
    "Elasticlunr",
    "Lodash"
  ],
  "author": "Grégory Tible, Anne L'Hôte, Michaël Goudoux, ByteClub",
  "license": "MIT",
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.5",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-preset-env": "^1.6.1",
    "bower": "^1.8.8",
    "browser-sync": "^2.26.3",
    "css-loader": "^0.28.10",
    "eslint": "^4.18.1",
    "gulp": "^3.9.1",
    "gulp-clean-css": "^3.10.0",
    "gulp-concat": "^2.6.0",
    "gulp-less": "^3.5.0",
    "gulp-ng-annotate": "^2.1.0",
    "gulp-uglify": "^3.0.1",
    "html-minify-loader": "^1.4.0",
    "maildev": "^1.0.0-rc3",
    "nodemon": "^1.18.9",
    "npm-run-all": "^4.1.5",
    "prettier-eslint-cli": "^4.7.1",
    "raw-loader": "^0.5.1",
    "style-loader": "^0.21.0",
    "uglify-es": "^3.3.9",
    "webpack": "^3.11.0",
    "webpack-bundle-analyzer": "^2.13.1"
  },
  "dependencies": {
    "angular": "1.7.5",
    "angular-diff-match-patch": "0.6.23",
    "angular-route": "1.7.5",
    "angularjs-dragula": "2.0.0",
    "babyparse": "^0.4.6",
    "chalk": "^2.4.2",
    "commonmark": "^0.28.1",
    "config": "^1.31.0",
    "connect-redis": "^3.4.0",
    "cors": "^2.8.5",
    "debug": "^3.2.6",
    "express": "^4.16.4",
    "express-boom": "^2.0.0",
    "express-fileupload": "^0.4.0",
    "express-session": "^1.15.6",
    "gulp-replace": "^0.6.1",
    "lodash": "^4.17.11",
    "mongoose": "^4.13.21",
    "natives": "^1.1.6",
    "ng-file-upload": "12.2.13",
    "nodemailer": "^4.7.0",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0",
    "rfc6902": "^2.4.0",
    "simplemde": "^1.11.2",
    "timeago.js": "^3.0.2",
    "toml": "^2.3.5",
    "yargs": "^11.1.0"
  }
}
