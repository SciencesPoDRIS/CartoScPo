{
  "name": "bib",
  "version": "0.0.2",
  "description": "Cartographie des centres de recherche en Sciences Politiques, réalisé par la bibliothèque et le médialab de Sciences Po Paris ainsi que ByteClub",
  "main": "index.html",
  "scripts": {
    "start": "gulp",
    "lint": "eslint app",
    "parse": "npm-run-all -p parse:*",
    "parse:data": "node app/data/script_data_csv_to_json",
    "parse:metadata": "node app/data/script_metadata_csv_to_json",
    "bo:watch": "npm-run-all -p bo:watch:*",
    "bo:watch:client": "webpack -d",
    "bo:watch:server": "DEBUG=express,mongoose nodemon --watch server server/index.js",
    "bo:serve": "DEBUG=express node server/index.js",
    "bo:populate": "node bin/populate-mongo-db.js",
    "bo:dump": "node bin/dump-mongo-db.js",
    "docker": "UID=$(id -u) GID=$(id -g) docker-compose up"
  },
  "keywords": [
    "Science",
    "Politque",
    "Bibliothèque",
    "SciencesPo",
    "médialab",
    "angular",
    "Leaflet",
    "Elasticlunr",
    "Lodash"
  ],
  "author": "Grégory Tible, Anne L'Hôte, Michaël Goudoux, ByteClub",
  "license": "MIT",
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.1",
    "browser-sync": "^2.13.0",
    "css-loader": "^0.28.7",
    "eslint": "^4.13.0",
    "gulp": "^3.9.1",
    "gulp-clean-css": "^3.9.0",
    "gulp-concat": "^2.6.0",
    "gulp-less": "^3.1.0",
    "gulp-ng-annotate": "^2.0.0",
    "gulp-uglify": "^3.0.0",
    "html-minify-loader": "^1.3.0",
    "nodemon": "^1.12.5",
    "npm-run-all": "^4.1.2",
    "raw-loader": "^0.5.1",
    "style-loader": "^0.19.0",
    "webpack": "^3.10.0"
  },
  "dependencies": {
    "angular": "^1.6.7",
    "angular-diff-match-patch": "^0.6.10",
    "angular-route": "^1.6.7",
    "angularjs-dragula": "^2.0.0",
    "babyparse": "^0.4.6",
    "commonmark": "^0.28.1",
    "config": "^1.28.1",
    "connect-redis": "^3.3.3",
    "debug": "^3.1.0",
    "express": "^4.16.2",
    "express-boom": "^2.0.0",
    "express-session": "^1.15.6",
    "lodash": "^4.11.1",
    "mongoose": "^4.13.7",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0",
    "timeago.js": "^3.0.2",
    "toml": "^2.3.3"
  }
}
